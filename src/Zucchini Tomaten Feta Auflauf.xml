<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="rezept.xsl"?>
<recipeml version="0.5">
  <recipe>
    <head>
      <title>Zucchini Tomaten Feta Auflauf</title>
    </head>    
    <yield>
		<qty>2</qty>
		<unit>Person</unit>
	</yield>
    <ingredients>
	  <ing> <qty>1</qty> <unit></unit> <item>Zucchini</item> </ing>
	  <ing> <qty>500</qty> <unit>g</unit> <item>Tomaten</item> </ing>
	  <ing> <qty>200</qty> <unit>g</unit> <item>Feta</item> </ing>
	  <ing> <qty></qty> <unit></unit> <item>Basilikum</item> </ing>
	  <ing> <qty></qty> <unit></unit> <item>Salz</item> </ing>
	  <ing> <qty></qty> <unit></unit> <item>Pfeffer</item> </ing>
	  <ing> <qty></qty> <unit></unit> <item>Oregano</item> </ing>
    </ingredients>
    <directions>
      <step>Zucchini, Tomaten, Feta in Scheiben schneiden.</step>
	  <step>Zucchini, Tomaten, Feta abwechselnd senkrecht in eine Auflaufform schichten.</step>
	  <step>20 Minuten bei 180 Grad Umluft backen.</step>
    </directions>
	<tags>
		<tag>vegetarisch</tag>
	</tags>
  </recipe>
</recipeml>

