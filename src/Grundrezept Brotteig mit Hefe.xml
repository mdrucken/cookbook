<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="rezept.xsl"?>
<recipeml version="0.5">
  <recipe>
    <head>
      <title>Grundrezept Brotteig mit Hefe</title>
    </head>    
    <yield>
		<qty>1</qty>
		<unit>Portion</unit>
	</yield>
    <directions>
      <step>Zutaten mischen.</step>
      <step>24 Stunden bei Raumtemperatur reifen lassen. Alle 8 Stunden dehnen und falten.</step>
      <step>Teig formen.</step>
      <step>1 Stunde refien lassen, mit einem Tuch abdecken.</step>
      <step>Teig backen.</step>
    </directions>
	<tags>
		<tag></tag>
	</tags>
  </recipe>
</recipeml>

