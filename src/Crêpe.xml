<recipeml version="0.5">
  <recipe>
    <head>
      <title>Crêpe</title>
    </head>    
    <yield>
		<qty>1</qty>
		<unit>Person</unit>
	</yield>
    <ingredients>
		<ing> <qty>1</qty> <unit></unit> <item>Ei</item> </ing>
		<ing> <qty>100</qty> <unit>g</unit> <item>Mehl</item> </ing>
		<ing> <qty>1</qty> <unit>EL</unit> <item>Zucker</item> </ing>
		<ing> <qty>200</qty> <unit>ml</unit> <item>Milch</item> </ing>
		<ing> <qty>1</qty> <unit>Prise</unit> <item>Salz</item> </ing>
		<ing> <qty></qty> <unit></unit> <item>Öl</item> </ing>
    </ingredients>
    <directions>
      <step>Ei mit 1 EL Zucker und 200ml Milch verrühren. Salz und Mehl dazu geben.</step>
	  <step>Teig 15 Minuten quellen lassen.</step>
	  <step>In der Pfanne ausbacken.</step>
    </directions>
